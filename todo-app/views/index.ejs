<html>
    <head>
        <title>My Todo Application</title>
        <script src="https://cdn.tailwindcss.com"></script>
        <link href="/css/styles.css" rel="stylesheet"/>
    </head>
    <body>
        <%- include ('header.ejs') %>
        <table>
            <% for(var i=0; i < allTodos.length; i++){%>
            <tr>
                <td>
                    <%= allTodos[i].title %>
                </td>
            </tr>
        <% } %>
        </table>
    </body>
</html>